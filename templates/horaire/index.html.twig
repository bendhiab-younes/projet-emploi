{% extends 'base.html.twig' %}

{% block title %}Horaire index{% endblock %}

{% block body %}
    <h1>Horaires</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>HeureDebut</th>
                <th>HeureFin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for horaire in horaires %}
            <tr>
                <td>{{ horaire.id }}</td>
                <td>{{ horaire.heureDebut }}</td>
                <td>{{ horaire.heureFin }}</td>
                <td>
                    <a href="{{ path('app_horaire_show', {'id': horaire.id}) }}">show</a>
                    <a href="{{ path('app_horaire_edit', {'id': horaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_horaire_new') }}">Create new</a>
{% endblock %}
